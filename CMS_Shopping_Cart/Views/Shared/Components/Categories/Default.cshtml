@model IEnumerable<Category>
<ul class="list-group">
    <li class="list-group-item">
        <a class="nav-link text-dark test" href="/products/index">
            All Productsssss
        </a>
    </li>

</ul>

@foreach (var item in Model)
{
    <li class="list-group-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Products" asp-action="ProductsByCategory" asp-route-categorySlug="@item.Slug">
            @item.Name
        </a>
    </li>
}